<script language="JavaScript" type="text/JavaScript">
    	$(document).ready(function(){  
				$(".btn_popup").PicButton();
				
	});
function myclick(index){
	if(index==1){
		$.webUtil.submitOnBtn("../ok.html",$('#btn1'));
	}else if(index==2){
			$.webUtil.submit("../ok.html");
	}else if(index==3){
			$.webUtil.open("../ok.html");
		
	}
	
	
}
</script>
