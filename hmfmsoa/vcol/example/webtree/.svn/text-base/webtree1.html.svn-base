function selNode(node){
	//alert("node="+node.html());
	
}

$(document).ready(function(){	
 	var tree1=$('#mytree1');
 	var tree2=$('#mytree2');
 	var tree3=$('#mytree3');
	tree1.webTree({collapsed:true,enable_selbtn:false,onSelect:selNode,onExpand:selNode,onCollapse:selNode});// 
	tree2.webTree({collapsed:true,enableIcon:true,enable_selbtn:true});// 
	tree3.webTree({collapsed:true,enableIcon:true,enable_selbtn:true,selbtn:{type:"radio"}});// 
	$('#mytree4').webTree({collapsed:true,enableIcon:true,icons:{type:1},enable_selbtn:true});// 
	$(".btn").PicButton();
	$("#btn11").click(function(){
		tree1.addNode($("<li><div>my child node!</div></li><li><div>my child node"+$(".nodeitem",tree1).length+"!</div></li>"),tree1.getSelectedNode());
		return false;
	});
	$("#btn12").click(function(){
		tree1.addNode($("<li><div>my brother node!</div></li><li><div>my brother node"+$(".nodeitem",tree1).length+"!</div></li>"),tree1.getSelectedNode(),false);
		return false;
	});
	$("#btn21").click(function(){
		tree2.addNode($("<li><div>my child node!</div></li><li><div>my child node"+$(".nodeitem",tree2).length+"!</div></li>"),tree2.getSelectedNode());
		return false;
	});
	$("#btn22").click(function(){
		tree2.addNode($("<li><div>my brother node!</div></li><li><div>my brother node"+$(".nodeitem",tree2).length+"!</div></li>"),tree2.getSelectedNode(),false);
		return false;
	});
	$("#btn23").click(function(){
		tree2.removeNode(tree2.getSelectedNode());
		return false;
	});
 
});		
	
</script>
