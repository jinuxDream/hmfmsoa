<script type="text/javascript">



$(document).ready(function(){	
	$('.btn').PicButton();
	var bar=$('.btn_toolbar').BtnToolBar();//初始化按钮工具条
	var g=$('.grid1').datagrid({toolbar:bar});
	var g=$('.grid2').datagrid({sortable:true,filterable:true});
	$("#myfilter").change(function(){
		var selVal =$(this).val();
		if(selVal=="all") selVal="";
		$('.grid1').setGridDisplayRow({btn_condition:selVal});
	});
});		
	
</script>
