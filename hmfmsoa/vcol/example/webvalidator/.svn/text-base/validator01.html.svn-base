<script type="text/javascript">
	$(document).ready(function(){	
		
	$('.grid_nohead').datagrid({hashead:false,trselect:false});////不需要表头，并且不需要行选择的设置
	$('.btn').PicButton( );
	$("#batch_no").setTipMessage("请输入您的申请编号");
	$("#sect_remark").setTipMessage("在此输入描述信息，信息格式为：维修的内容，施工工艺");

	$("#form1").setValidator();
	//$("#sect_file").setValidator({empty:false});
	$.webUtil.onKeyDownEnter($(document),$("#btn_save"));
	



		
		
		
});		
function mySubmit(btn){
	//some process..........
	$.webUtil.submitOnBtn("../ok.html",$(btn));
	
}
function formatNumfunction(b){
		var e={symbol:'A'},c;
		var decSymbol=".";
	
			c=new RegExp("[^\\d"+decSymbol+"-]","g")
		
		b=b.replace(c,"");
			b=b.replace(decSymbol,"$#$").replace(new RegExp("\\.","g"),"").replace("$#$",decSymbol);
			b=b.replace(/^\-/g,"$*$").replace(/\-/g,"").replace("$*$","-");
			b=b.replace(new RegExp("^\\-$|^-"+decSymbol+"$|^\\"+decSymbol+"$|^-"+decSymbol+"\\d$"),"");
			return b
	}
</script>

</head>

