<head>
<meta http-equiv="Content-Type" content="text/html; charset=GBK" />
<title>webdialog1</title>
<link rel="stylesheet" type="text/css" href="../../style/web-ui.css" />
<link rel="stylesheet" type="text/css" href="../../style/your-custom.css" />
<script type="text/javascript" src="../../js/jquery-1.7.js"></script>
<script type="text/javascript" src="../../js/web-ui.js"></script>
<script type="text/javascript">

var dlg;
$(document).ready(function(){	
	$('.btn').PicButton();
	$('.grid_nohead').datagrid({trselect:false,hashead:false});
	var g=$('.grid1').datagrid( {width:'90%',trselect:false});//或执行代码 g[0].grid.setBar(bar);	
  //dlg=$("body").WebDialog();
	$("#btn1").click(function(e ){
		$.webUtil.alert("请您确认一下！！！！！",function(){ alert("你确认后，回调到自定义的函数啦！");});
	});
	$("#btn2").click(function(e ){
		$.webUtil.confirm("您确定要xxx吗？",
		function(){ alert("看来你确定要xxx了，回调你的确认函数！");},
		function(){ alert("你取消了要xxx，回调你的取消函数！");}
	);
	});
	$("#btn3").click(function(e ){
		$.webUtil.openWindow(
			{title:"自定义按钮",buttons:[
				{btn:'<button>我的按钮1</button>',onclick:function(e){$(e.data.target).closeWebDialog();}},
				{btn:'<button>我的按钮2</button>',onclick:function(e){$(e.data.target).closeWebDialog();}},
				{btn:'<button>我的按钮3（提交表单）</button>',onclick:function(e){
					$.webUtil.submit("../ok.html");
					$(e.data.target).closeWebDialog();
				}
				}
			]});
	});
	$("#btn4").click(function(e ){
		alert($("#dlg_btn1").length);
		$.webUtil.openWindow(
			{title:"自定义按钮",buttons:[
				{btn:$("#dlg_btn1")},
				{btn:$("#dlg_btn2"),onclick:function(e){$(e.data.target).closeWebDialog();}}
 			]});
	});
	$("#btn5").click(function(e ){
		$.webUtil.openWindow(
			{title:"自定义按钮",buttons:$(".fordialog2 .btn")});
	});
/////////////////////

	$(".testdlg").click(function(e){
		$.webUtil.openWindow({event:e});	
	});
/////////////	
	
	

});		
 	
</script>

</head>

