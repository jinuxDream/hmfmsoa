/* reset */
body, div, span, p, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, em, img, ins, s, samp, small, strike, strong, sub, sup, tt, var, dd, dl, dt, li, ol, ul, fieldset, label, legend, table, caption, tr, th, td, form, input, select, textarea { margin:0;padding:0;}
img{ border:0;}
ul,ol{ list-style:none;}
h1,h2,h3,h4,h5,h6{font-weight:700;}
h1{font-size:18px}
h2{font-size:16px}
h3{font-size:14px}
h4{font-size:14px}
h5{font-size:12px}
h6{font-size:12px}
hr {margin: 0;padding: 0;border: 0;color: #CDCDCD;background-color: #CDCDCD;height: 1px}
address,caption,cite,code,dfn,em,th,var{font-weight:normal;font-style:normal}
strong, b{ font-weight: bold; }
table { border-collapse:collapse; border-spacing:0;}
caption,th { text-align:left; font-weight:normal}
button,input,select,textarea { font-size:12px; font-family:"宋体", "Arial", "Times New Roman";resize:none;}
:focus{ outline:none}
pre{
	white-space: pre-wrap;       /* CSS-3 */
	white-space: -moz-pre-wrap;  /* Mozilla, since 1999 */
	white-space: -pre-wrap;      /* Opera 4-6 */
	white-space: -o-pre-wrap;    /* Opera 7 */
	word-wrap: break-word;       /* Internet Explorer 5.5+ */
}
table{border-collapse:collapse}
td{font-size:12px;}

body {
 font-family: "宋体", "Arial", "Times New Roman";
