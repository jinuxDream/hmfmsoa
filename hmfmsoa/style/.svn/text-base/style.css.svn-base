/*tabs*/
.tabs { position: static;} 
.tabs ul { margin: 0; padding: .2em 0 0; position: relative; background: #5c9ccc url("../images/tabs.png") 50% 50% repeat-x; height:30px; height:27px\9; border:1px solid #5c9ccc !important;  border-bottom:0;}
.tabs ul li { list-style: none; float: left;position:relative; margin-right: 3px; border-bottom: 0 !important; padding: 0; white-space: nowrap;cursor: pointer;}
.tabs ul li a { float: left; padding: .5em 1em; text-decoration: none; border: 1px solid #79b7e7; background: #d0e5f5 url(../images/tabs_glass.png) 50% 50% repeat-x; font-size:12px;font-weight: bold; color: #2972AF;}
.tabs ul li a:hover{background-color: #EAF4FD;text-decoration: none;color: #2972AF;border: 1px solid #5C9CCC;filter:alpha(opacity=80);-moz-opacity:0.8; opacity:0.8;cursor: pointer;}
.tabs ul li a.selected{background-color: #ffffff;color:#E57009;border: 1px solid #79b7e7; border-bottom: 1px solid #ffffff;background-image: none;filter:alpha(opacity=100);-moz-opacity:1; opacity:1;cursor: text;}
.tabs .tabs-panel{clear: both;padding:5px;}

/*paper of word file*/
.paper{border:1px dashed #808080; position:relative;z-index:2;padding:10px 40px;background-color: #FCFCFC;-moz-box-shadow: 0 2px 10px 1px rgba(0, 0, 0, 0.2);-webkit-box-shadow: 0 2px 10px 1px rgba(0, 0, 0, 0.2);box-shadow: 0 2px 10px 1px rgba(0, 0, 0, 0.2);}
.paper h1{font-size:16px;text-align: center;}
.paper .p_close{background: url("../images/panel-close.gif"); cursor: pointer;width: 7px; height: 7px; display: block; float: right; margin-right: 10px;}
.paper .underline{font-size: 13px;border:none; border-bottom: 1px solid #AAA;background-color:transparent;padding: 0 5px;}

.paper .paper-table{width:100%;margin: 10px 0; table-layout: fixed;}
.paper .paper-table td{border: 1px solid #000;padding: 5px;font-size:13px; word-wrap: break-word; word-break:break-all;}
.paper .paper-table td.title{text-align: center;font-family: "仿宋体"; font-weight: bold;}
.paper .paper-table td input,.paper-table select{font-size: 13px;width:100%; border: 1px solid #AAA;background-color:transparent;}
.paper .paper-table td textarea{width:100%;height:80px;resize:none;border: 0px solid #eee;background-color:transparent;overflow: auto;}

.paper .paper-tab{width:100%; margin: 10px 0;}
.paper .paper-tab tr{height:30px;}
.paper .paper-tab td{border: 0px solid #000;padding: 5px;font-size:13px; word-wrap: break-word; word-break:break-all;}
.paper .paper-tab td.title{text-align: center;font-family: "仿宋体"; font-weight: bold;}
.paper .paper-tab input,.paper-tab select{font-size: 13px;width:100%;border:none; border-bottom: 1px solid #AAA;background-color:transparent;}
.paper .paper-tab input.radio, .paper .paper-tab td input.checkbox{width: auto;border: none;vertical-align: middle;}
.paper .paper-tab textarea{width:100%;height:60px;resize:none;border:none; border: 1px solid #AAA;background-color:transparent;overflow-y: auto;}
.paper .inner-tab{width:100%;}
.paper .inner-tab td,.paper .inner-tab th{border: 1px solid #AAA;padding: 4px;font-size:12px;}
.paper .inner-tab th{text-align: center;font-size:13px;}
.paper .inner-tab input.radio, .paper .inner-tab input.checkbox{width: auto;border: none;vertical-align: middle;background: red}

.paper .paper-text p{font-size: 15px; line-height: 28px;}
